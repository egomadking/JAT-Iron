# Ignore bundler config.
jat-iron-api/.bundle

# Ignore all logfiles and tempfiles.
jat-iron-api/log/*
jat-iron-api/tmp/*
!jat-iron-api/log/.keep
!jat-iron-api/tmp/.keep

# Ignore pidfiles, but keep the directory.
jat-iron-api/tmp/pids/*
!jat-iron-api/tmp/pids/
!jat-iron-api/tmp/pids/.keep

# Ignore uploaded files in development.
jat-iron-api/storage/*
!jat-iron-api/storage/.keep
jat-iron-api.byebug_history

# Ignore master key for decrypting credentials and more.
jat-iron-api/config/master.key

